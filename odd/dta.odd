    <text>
        <body>
            <schemaSpec start="TEI teiCorpus" ident="dta" source="teipublisher.odd">
                <elementSpec ident="pb" mode="change">
                    <model output="epub" behaviour="break">
            <param name="type" value="'pagebreak'"/>
            <param name="label" value="@n/string()"/>
        </model>
                    <model predicate="count(../*) = 1 and count(ancestor::*) = 1" behaviour="inline" useSourceRendition="true">
            <param name="content" value="'[Empty page]'"/>
            <outputRendition xml:space="preserve">
            color: #CCC;
            </outputRendition>
        </model>
                    <model behaviour="omit"/>
                </elementSpec>
                <elementSpec ident="fw" mode="change">
                    <model output="web" predicate="@place='top' and $parameters?view='page'" behaviour="inline">
            <outputRendition xml:space="preserve">
            
            
                            
                            display: inline-block;
                            margin-top: .5em;
                            margin-bottom: 1em;
                            color: #CCC;
                            
                        
            
            </outputRendition>
        </model>
                    <model output="web" predicate="@type='sig' and $parameters?view='page'" behaviour="block">
            <outputRendition xml:space="preserve">
            
            
                            
                            
                            
                            margin-top: 1em;
                            color: #A0A0A0;
                            text-align: left;
                            
                            
                            
                        
            
            </outputRendition>
        </model>
                    <model output="web" predicate="@type='catch' and $parameters?view='page'" behaviour="block">
            <outputRendition xml:space="preserve">
            
            
                            
                            
                            
                            margin-left: 20em;
                            color: #A0A0A0;
                            
                            
                            
                        
            
            </outputRendition>
        </model>
                    <model behaviour="omit"/>
                </elementSpec>
                <elementSpec ident="lb" mode="change">
                    <modelSequence output="web" predicate="@break='no' and $parameters?view='page'">
            <model behaviour="inline">
                <param name="content" value="'-'"/>
            </model>
            <model behaviour="break">
                <param name="type" value="'line'"/>
                <param name="label" value="@n"/>
            </model>
        </modelSequence>
                    <model output="web" predicate="$parameters?view='page' and not(following-sibling::*[1][self::p|self::div])" behaviour="break" useSourceRendition="true">
            <param name="type" value="'line'"/>
            <param name="label" value="@n"/>
        </model>
                    <model behaviour="omit"/>
                </elementSpec>
            </schemaSpec>
