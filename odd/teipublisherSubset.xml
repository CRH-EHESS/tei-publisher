    <text>
        <body>
            <schemaSpec ident="teipublisher" source="tei_simplePrint.odd" start="TEI teiCorpus"><elementSpec rend="add" module="derived-module-teipublisher" ident="titleStmt" mode="add"><model behaviour="meta" output="latex">
                <param name="content" value="."/>
            </model><model behaviour="heading" output="fo"/><modelSequence output="web" predicate="$parameters?header='short'">
                <model behaviour="link">
                    <param name="content" value="title[1]"/>
                    <param name="link" value="$parameters?doc"/>
                </model>
                <model behaviour="block">
                    <param name="content" value="subsequence(title, 2)"/>
                </model>
                <model behaviour="block">
                    <param name="content" value="author"/>
                </model>
            </modelSequence><model behaviour="block" output="web"/></elementSpec><elementSpec rend="add" module="derived-module-teipublisher" ident="publicationStmt" mode="add"><model behaviour="block" output="web">
                <param name="content" value="availability/licence"/>
            </model><model behaviour="omit"/></elementSpec><elementSpec rend="add" module="derived-module-teipublisher" ident="licence" mode="add"><model behaviour="link" cssClass="licence" output="web" predicate="@target">
                <param name="content" value="'Licence'"/>
                <param name="link" value="@target"/>
            </model><model behaviour="omit"/></elementSpec></schemaSpec>
        </body>
    </text>
